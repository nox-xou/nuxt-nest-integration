{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./.nest",
    "baseUrl": "./",
    "incremental": true
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    ".nest",
    "client"
  ]
}
